import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int rev = 0;
        int last;
        while(num>0){
            last = num%10;
            rev = (rev*10) +last;
            num/=10;
        }
        System.out.println(rev);
        
    }
}
